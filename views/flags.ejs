    <link rel="stylesheet" href="style.css"/>
</head>
<body>
    <%- include('partials/nav.ejs') %>
    <main>
        <% for( let i = 0; i < countryID.length; i++ ) { %>
        <div class="Flags">
            <%if(countryID[i] != "XK") {%>
            <img draggable="false" src="https://flagsapi.com/<%= countryID[i]%>/flat/64.png">
            <%  %>
            <input type="text" name="inputFlag" class="inputFlag" id="<%=countryID[i]%>">
            <% } %>
        </div>
        <%} %>
        </main>
        <script src="flags.js"></script>
    </body>
</html>